<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="bottomPane" fx:id="bottomPane" prefHeight="100.0" prefWidth="200.0" spacing="5.0" stylesheets="/css/StyleBottomPane.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.main_controllers.MainPageController">
   <children>
      <VBox id="westPane" prefHeight="200.0" prefWidth="100.0" spacing="3.0" HBox.hgrow="ALWAYS">
         <children>
            <Button fx:id="btnFlights" mnemonicParsing="false" onAction="#showFlights" text="FLIGHTS" />
            <Button fx:id="btnPrices" mnemonicParsing="false" onAction="#showPrices" text="PRICES" />
            <Button mnemonicParsing="false" text="PLAN YOUR FLIGHT" />
            <Button mnemonicParsing="false" text="AIRLINES" />
            <Button mnemonicParsing="false" text="CHECK IN" />
            <Button mnemonicParsing="false" onAction="#showPassengers" text="PASSENGERS" />
            <Button mnemonicParsing="false" text="SEARCH" />
            <Button mnemonicParsing="false" text="EDIT DATA" />
         </children>
      </VBox>
      <VBox id="eastPane" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
